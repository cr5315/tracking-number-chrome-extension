{
    "manifest_version": 2,

    "name": "Tracking Number Quick-link",
    "description": "Quickly open the tracking page after highlighting a tracking number",
    "version": "1.0",
    
    "icons": {
        "16" : "icon_16.png",
        "19" : "icon_19.png",
        "48" : "icon_48.png",
        "128" : "icon_128.png"
    },

    "permissions": [
        "contextMenus"
    ],
    "background":{
        "scripts": ["tracking.js"]
    }
}